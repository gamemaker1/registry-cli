{
	"name": "registry-cli",
	"version": "0.2.0",
	"description": "A CLI to create and interact with Sunbird RC registry instances",
	"author": "Vedant K <https://github.com/gamemaker1>",
	"license": "MIT",
	"main": "source/main.ts",
	"type": "module",
	"bin": {
		"registry": "build/main.js"
	},
	"files": [
		"build",
		"source",
		"config",
		"pnpm-lock.yaml",
		"package.json"
	],
	"scripts": {
		"start": "node --no-warnings --loader ts-node/esm --es-module-specifier-resolution node source/main.ts",
		"test": "sleep 10",
		"build": "tsc -p config/typescript.json",
		"watch": "tsc -p config/typescript.json --watch"
	},
	"dependencies": {
		"chalk": "^4.1.2",
		"command-exists": "^1.2.9",
		"commander": "^8.2.0",
		"docker-compose": "^0.23.13",
		"dockerode": "^3.3.1",
		"git-clone": "^0.2.0",
		"got": "^11.8.2",
		"ora": "^6.0.1",
		"replace-in-file": "^6.2.0"
	},
	"devDependencies": {
		"@types/command-exists": "^1.2.0",
		"@types/dockerode": "^3.3.0",
		"@types/node": "^16.10.9",
		"ts-node": "^10.3.0",
		"typescript": "^4.4.4"
	}
}
